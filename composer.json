{
  "name": "jambagecom/base-cache",
  "type": "typo3-cms-extension",
  "description": "Common interfaces for simple caching by psr/simple-cache.",
  "keywords": [
    "TYPO3",
    "extension",
    "parser",
    "cache",
    "stylesheet"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_cache/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^10.4 || ^11.5 || ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Psr\\SimpleCache\\": "Vendor/simple-cache/src/"
    }
  },
  "replace": {
    "typo3-ter/base-cache": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_cache"
    }
  }
}
